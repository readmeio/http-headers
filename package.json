{
  "name": "http-headers",
  "version": "1.0.0",
  "description": "Retrieve HTTP Header Descriptions ",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "lint": "eslint . --ext .js,.ts",
    "lint:fix": "eslint . --ext .js,.ts --fix",
    "prebuild": "rm -rf dist",
    "prepack": "npm run build",
    "pretest": "npm run lint && npm run prettier",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --check --write .",
    "test": "vitest run --coverage",
    "test-watch": "vitest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/readmeio/http-headers.git"
  },
  "keywords": [
    "http"
  ],
  "author": "Gabe Ratcliff",
  "license": "MIT",
  "dependencies": {
    "mdast-util-from-markdown": "^2.0.0",
    "node-fetch": "^3.3.2",
    "unist-util-find": "^2.0.0",
    "unist-util-find-after": "^5.0.0",
    "unist-util-flat-filter": "^2.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.3",
    "@commitlint/config-conventional": "^17.6.3",
    "@readme/eslint-config": "^12.0.3",
    "@types/unist": "^3.0.0",
    "@vitest/coverage-v8": "^0.34.1",
    "eslint": "^8.40.0",
    "nock": "^13.3.2",
    "prettier": "^3.0.1",
    "typescript": "^5.0.4",
    "vitest": "^0.34.1"
  },
  "prettier": "@readme/eslint-config/prettier"
}
